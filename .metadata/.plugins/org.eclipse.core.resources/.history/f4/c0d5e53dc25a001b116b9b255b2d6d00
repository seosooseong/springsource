/**
 * 
 */
$(function() { 
	$(function() {
			$("button").click(function(e) {
				e.preventDefault();//submit버튼 일단 막기
				console.log("product 보내기...");

				$.ajax({
					url : '/product',
					type : 'post',
					//processData : false, 
					//contentType : false,
					data : $("form").serialize(),
					success : function(result) {
						console.log(result);
						
					}
				})
				
			})
		})
})